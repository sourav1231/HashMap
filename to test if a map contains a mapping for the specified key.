import java.util.*;
class Hashmapp
{ 
  public static void main(String[] args)
  {
    //creating first hashMap of student id and student name
    HashMap<String,Integer> student=new HashMap<String,Integer>();
    student.put("ram",1);
    student.put("shyam",2);
    student.put("ghanshaym",3);
    student.put("rahul",4);
        System.out.println("hashmap contains: "+student);
       if(student.containsKey("shyam"))
       {
           System.out.println("yes bro it contains");
       }
       else{
           System.out.println("no bro");
           
       }
  }
}
