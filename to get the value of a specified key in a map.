import java.util.*;
class Hashmapp
{ 
  public static void main(String[] args)
  {
    //creating first hashMap of student id and student name
    HashMap<String,Integer> student=new HashMap<String,Integer>();
    student.put("ram",1);
    student.put("shyam",2);
    student.put("ghanshaym",3);
    student.put("rahul",4);
       Integer value=student.get("shyam");
        System.out.println("value at shyam is:"+value);
  }
}
