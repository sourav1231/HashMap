import java.util.*;
class Hashmapp
{ 
  public static void main(String[] args)
  {
    //creating first hashMap of student id and student name
    HashMap<String,Integer> student=new HashMap<String,Integer>();
    student.put("ram",1);
    student.put("shyam",2);
    student.put("ghanshaym",3);
    student.put("rahul",4);
        System.out.println("first hashmap contains: "+student);
    //creating second hashmap
    HashMap<String,Integer> student1=new HashMap<String,Integer>();
    student1.put("vikash",5);
    student1.put("ashish",6);
    student1.put("samir",7);
    student1.put("sammy",8);
        System.out.println("second hashmap contains: "+student1);
        
        //coping all the key,value pairs of secomd hashMap into first HashMap
        student.putAll(student1);
    //now first hashmap contains 
         System.out.println("now first hashmap contains: "+student);
  }
}
